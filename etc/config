# 游戏文件夹路径
export GamePath="${HOME}/Minecraft"
# 会话名(若在修改时任有服务端在运行，会导致残留进程)
export ScreenName='Minecraft[1.18.2] Java'
# 启动时内存(MB)
export StartCache=2048
# 最大内存(MB)
export MaxCache=4096
# 你要连接的中转主机的地址是什么
export MasterHost=master
# 启用SSL(https)?
export HostProtocol=https
# 是否忽略可能不安全的链接
# 如果你的中转主机SSL证书与这个域名不匹配或者不由某官方机构颁发就填true
export Insecure=false
# 是否启用第三方验证
export Authlib=false
# 第三方验证的验证服务地址
export AuthlibInjector="https://littleskin.cn/api/yggdrasil"
# 第三方验证器版本
export AuthlibInjectorVer="5.1.1"
# 服务器启动核心文件名(不含后缀)
export MainJAR="spigot-1.18.2"
# 服务器核心,备份文件时使用,支持的值为以下列表
# 模式名称:等效的值
# official:official,forge
# unofficial:unofficial,bukkit,spigot,paper,purpur,airplane
# 注: 要启用备份配置文件功能请在Backup目录下创建Config文件夹
export ServerCore="spigot"
# 备份所使用的线程数量(如果因为线程过多被杀, 请调回来)
export BackupThread=1
# 备份文件的压缩质量(调整至更大会占用相当大的内存空间)
# 可选的值: 0~9
export BackupCompressLevel=6
# 离开时等待的秒数(若是超过此时间服务器还没停止, 就会强制杀死进程)
export StopWaitTimeMax=20

# minecraftctl显示的语言
export Language="zh-CN"
# minecraftctl安装路径
export InstallPath='/opt/minecraftctl'
# 配置刹车时长(秒), 避免长时间过高的CPU占用
export SleepSecond=15
# 配置工作时长(秒)
export WorkSecond=15
# 设置切片超时时间(秒), 如果完成切片的时间超过这个值就放弃刹车
export WorkExceedSecond=200
# 配置每个工作切片周期(行)
export WorkPart=1000
# 限制每个命令的最大运行时间(秒)
export CommandMaxRun=5